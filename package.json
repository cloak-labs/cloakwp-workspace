{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --continue --filter=*cloakwp* --filter=@inception/backend --filter=@inception/frontend & pnpm browser-sync",
    "browser-sync": "npx browser-sync start --config bs-config.js",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=cloakwp... && changeset publish",
    "wp-env": "wp-env"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@cloakwp/eslint-config": "workspace:*",
    "@wordpress/env": "^6.0.0",
    "browser-sync": "^2.29.3",
    "eslint": "^7.32.0",
    "meta-npm": "^1.2.7",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}
