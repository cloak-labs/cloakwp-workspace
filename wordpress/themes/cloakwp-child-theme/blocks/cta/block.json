{
  "name": "acf/cta",
  "title": "CTA",
  "description": "A custom block for displaying a CTA (call-to-action).",
  "textdomain": "cloakwp",
  "category": "widgets",
  "icon": "external",
  "keywords": [
    "cta",
    "call",
    "action"
  ],
  "acf": {
    "mode": "edit",
    "renderTemplate": "block-preview.php"
  },
  "align": false,
  "supports": {
    "anchor": true,
    "mode": true,
    "align": false,
    "html": false,
    "reusable": false,
    "lock": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    },
    "color": {
      "background": true,
      "gradients": false,
      "text": false
    }
  },
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "margin": {
            "top": "80px",
            "bottom": "80px"
          }
        }
      }
    }
  }
}