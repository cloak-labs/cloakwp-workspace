{
  "name": "@cloakwp/spinup-local-wp",
  "version": "0.0.2",
  "description": "Spin up your local WordPress/Bedrock instance with zero-config, using Docker under the hood.",
  "license": "MIT",
  "scripts": {
    "zzz-dev": "docker-compose up -d",
    "zzz-stop": "docker-compose down",
    "zzz-composer": "docker-compose run composer",
    "zzz-postinstall": "docker-compose run composer create-project"
  },
  "bin": {
    "spinup-local-wp": "./bin/spinup-local-wp.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloak-labs/cloakwp.git"
  },
  "keywords": [
    "wordpress",
    "wp",
    "local",
    "bedrock",
    "roots",
    "cloakwp",
    "cloak"
  ],
  "author": "CloakWP",
  "bugs": {
    "url": "https://github.com/cloak-labs/cloakwp/issues"
  },
  "homepage": "https://github.com/cloak-labs/cloakwp/tree/main/packages/spinup-local-wp#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "yargs": "^17.7.2"
  }
}
