{
  "prviate": true,
  "name": "@basic-starter/backend",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "spinup-local-wp dc up",
    "dev:rebuild": "spinup-local-wp dc up -d --force-recreate --build",
    "stop": "spinup-local-wp dc down",
    "composer": "spinup-local-wp dc run composer",
    "postinstall": "pnpm composer create-project",
    "spinup-local-wp": "spinup-local-wp",
    "generate-env": "php -r \"copy('.env.example', '.env');\""
  },
  "dependencies": {
    "@cloakwp/spinup-local-wp": "workspace:*"
  }
}